<div class="container">
<%= form_with(model: car, local: true) do |form| %>
  <% if car.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(car.errors.count, "error") %> prohibited this car from being saved:</h2>

      <ul>
      <% car.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="row">
    <div class="form-group col-xs-4">
    <%= form.label :brand %>
    <%= form.select :typeof,[["BMW"],["Mercedes"],["Audi"],["Volkswagen"],["Porsche"]],{}, class: "form-control"%>
    </div>

    <div class="form-group col-xs-4">
    <%= form.label :color %>
    <%= form.text_field :color, class: "form-control"%>
    </div>
  </div>

  <div class="row">
    <div class="form-group col-xs-4">
    <%= form.label :hp %>
    <%= form.number_field :hp, class: "form-control"%>
    </div>

    <div class="form-group col-xs-4">
    <%= form.label :typeof %>
    <%= form.select :typeof,[["Benzin"],["Diesel"]],{}, class: "form-control"%>
    </div>
  </div>

  <div class="actions">
    <div class="form-group">
    <input type="submit" value="Send" class="btn btn-success" />
    </div>
  </div>
<% end %>
</div>